{
    "name": "@ravenapp/raven",
    "version": "0.0.35",
    "repository": "https://github.com/ravenappdev/raven-node",
    "files": [
        "core",
        "resources",
        "serialization",
        "client",
        "*.{js,js.map,d.ts}"
    ],
    "main": "./index.js",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "require": "./index.js",
            "import": "./index.js",
            "types": "./index.d.ts"
        }
    },
    "scripts": {
        "format": "prettier --write --print-width 120 'src/**/*.ts'",
        "build": "esbuild $(find src -name '*.ts') --format=cjs --sourcemap --outdir=. && tsc"
    },
    "dependencies": {
        "@types/url-join": "4.0.1",
        "axios": "^0.27.2",
        "url-join": "4.0.1"
    },
    "devDependencies": {
        "@types/node": "17.0.33",
        "esbuild": "0.14.47",
        "prettier": "2.7.1",
        "typescript": "4.6.4"
    },
    "packageManager": "yarn@3.2.4"
}
